<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:entviews="clr-namespace:Lumper.UI.Views.Bsp.Lumps.PakFile"
             xmlns:entmodels="clr-namespace:Lumper.UI.ViewModels.Bsp.Lumps.PakFile"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Lumper.UI.Views.Bsp.Lumps.PakFile.PakFileEntryView">

    <Grid RowDefinitions="Auto,*">
        <Button Grid.Row="0" Command="{Binding Open}">_Open</Button>
        <TextBox Grid.Row="1" Text="{Binding Content}"></TextBox>
    </Grid>
</UserControl>
