    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>..\Lumper.UI\Assets\Images\Lumper.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup>
    <PublishDir>../../publish/Lumper.CLI</PublishDir>
