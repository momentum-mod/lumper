    <PackageReference Include="NLog" Version="5.4.0" />
    <PackageReference Include="nulastudio.NetBeauty" Version="2.1.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Lumper.Lib\Lumper.Lib.csproj"/>
  </ItemGroup>

</Project>
